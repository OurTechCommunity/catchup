{
	"arrowParens": "always",
	"bracketSpacing": true,
	"cursorOffset": -1,
	"embeddedLanguageFormatting": "auto",
	"endOfLine": "auto",
	"filepath": "",
	"htmlWhitespaceSensitivity": "css",
	"insertPragma": false,
	"jsxSingleQuote": false,
	"overrides": [],
	"plugins": ["prettier-plugin-asciidoc"],
	"pluginSearchDirs": [],
	"printWidth": 80,
	"proseWrap": "preserve",
	"quoteProps": "preserve",
	"rangeEnd": -1,
	"rangeStart": 0,
	"requirePragma": false,
	"semi": true,
	"singleQuote": false,
	"tabWidth": 4,
	"trailingComma": "none",
	"useTabs": true,
	"vueIndentScriptAndStyle": false
}
